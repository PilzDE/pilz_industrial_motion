<testdata>

  <!-- See ../../concept_testdata.png for a visualization of the concept behind these points -->

  <poses>

    <pos name="ZeroPose">
        <group name="panda_arm">
            <joints>0.0 0.0 0.0 -1.5708 0.0 1.5708 0.0</joints>
        </group>
    </pos>

    <pos name="P1">
        <group name="panda_arm">
            <joints>0.675563 -0.516596 -0.647210 -2.571306 -0.3559277 2.116679 0.284819</joints>
            <xyzQuat link_name="panda_link8">0.4 0.0 0.4 1.0 0.0 0.0 0.0</xyzQuat>
        </group>
    </pos>

    <pos name="P2">
        <group name="panda_arm">
            <joints>0.864803 0.393524 -0.603271 -1.831867 0.262145 2.1453352 0.1533288</joints>
            <xyzQuat link_name="panda_link8">0.6 0.2 0.35 1.0 0.0 0.0 0.0</xyzQuat>
        </group>
    </pos>

    <pos name="P3">
        <group name="panda_arm">
            <joints>1.2565809 0.249040 -0.4810534 -2.141857 0.162130 2.357005 0.673026</joints>
            <xyzQuat link_name="panda_link8">0.4 0.4 0.3 1.0 0.0 0.0 0.0</xyzQuat>
        </group>
    </pos>

    <!-- Deprecated: Do not use this point anymore -->
    <!-- Equal to P1 -->
    <pos name="PTPPose">
        <group name="panda_arm">
            <joints>0.675563 -0.516596 -0.647210 -2.571306 -0.3559277 2.116679 0.284819</joints>
            <xyzQuat link_name="panda_link8">0.4 0.0 0.4 1.0 0.0 0.0 0.0</xyzQuat>
        </group>
    </pos>

    <!-- Deprecated: Do not use this point anymore -->
    <!-- Equal to P1 -->
    <pos name="PTPJointValid">
        <group name="panda_arm">
            <joints>0.675563 -0.516596 -0.647210 -2.571306 -0.3559277 2.116679 0.284819</joints>
        </group>
    </pos>

  </poses>


  <circs>

    <circ name="ValidCIRCCmd2">
      <!-- valid circ cmd-->
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P1</startPos>
      <centerPos>P2</centerPos>
      <endPos>P3</endPos>
      <vel>0.1</vel>
      <acc>0.1</acc>
    </circ>

    <circ name="circ3_interim">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P1</startPos>
      <intermediatePos>P2</intermediatePos>
      <endPos>P3</endPos>
      <vel>0.1</vel>
      <acc>0.4</acc>
    </circ>

    <circ name="circ1_center_2">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P3</startPos>
      <centerPos>P2</centerPos>
      <endPos>P1</endPos>
      <vel>0.1</vel>
      <acc>0.4</acc>
    </circ>

  </circs>


  <lins>

    <lin name="LINCmd1">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P1</startPos>
      <endPos>P2</endPos>
      <vel>0.3</vel>
      <acc>0.1</acc>
    </lin>

    <lin name="lin3">
        <planningGroup>panda_arm</planningGroup>
        <targetLink>panda_link8</targetLink>
        <startPos>P2</startPos>
        <endPos>P3</endPos>
        <vel>0.1</vel>
        <acc>0.2</acc>
    </lin>

    <lin name="lin2">
        <planningGroup>panda_arm</planningGroup>
        <targetLink>panda_link8</targetLink>
        <startPos>P1</startPos>
        <endPos>P2</endPos>
        <vel>0.1</vel>
        <acc>0.2</acc>
    </lin>

  </lins>

  <ptps>
    <ptp name="FirstPtp">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>prbt_tcp</targetLink>
      <startPos>ZeroPose</startPos>
      <endPos>PTPJointValid</endPos>
      <vel>1.0</vel>
      <acc>1.0</acc>
    </ptp>

    <ptp name="Ptp1">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>ZeroPose</startPos>
      <endPos>P1</endPos>
      <vel>1.0</vel>
      <acc>0.4</acc>
    </ptp>

    <ptp name="Ptp2">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P3</startPos>
      <endPos>P2</endPos>
      <vel>1.0</vel>
      <acc>0.4</acc>
    </ptp>

    <ptp name="Ptp3">
      <planningGroup>panda_arm</planningGroup>
      <targetLink>panda_link8</targetLink>
      <startPos>P2</startPos>
      <endPos>P3</endPos>
      <vel>1.0</vel>
      <acc>0.4</acc>
    </ptp>
  </ptps>

  <sequences>
    <blend name="ComplexSequence">
      <sequenceCmd name="Ptp1"            type="ptp_joint_cart"     blend_radius="0.1" />
      <sequenceCmd name="circ3_interim"   type="circ_interim_cart"  blend_radius="0.1" />
      <sequenceCmd name="Ptp2"            type="ptp_cart_cart"      blend_radius="0.1" />
      <sequenceCmd name="lin3"            type="lin_cart"           blend_radius="0.1" />
      <sequenceCmd name="circ1_center_2"  type="circ_center_cart"   blend_radius="0.1" />
      <sequenceCmd name="lin2"            type="lin_cart"           blend_radius="0.1" />
      <sequenceCmd name="Ptp3"            type="ptp_cart_cart"      blend_radius="0.0" />
    </blend>
  </sequences>

</testdata>
